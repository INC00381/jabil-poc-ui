<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:u="sap.ui.unified" xmlns:ui="sap.ui.table"
	controllerName="JabilPoc.Jabil_Poc.controller.bulkUpdate" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Title" showHeader="false">
				
				<HBox class="uploadContainerClass sapUiSizeCompact">
					<Button icon="sap-icon://nav-back" press="onBack"></Button>
					<u:FileUploader  name="myFileUpload" uploadUrl="upload/" tooltip="Upload your file to the local server"
						uploadComplete="handleUploadComplete"/>
					<Button text="Upload File" press="onUpload"/>
				</HBox>
				<ui:Table id="table1" class="materialTableClass sapUiSizeCompact"
					rows="{oBulkUploadModel>/listRequestDto}" selectionMode="MultiToggle" fixedColumnCount="1" visibleRowCount="14" ariaLabelledBy="title">
					<!--<ui:toolbar>-->
					<!--	<Toolbar>-->
					<!--		<content>-->
					<!--			<Title id="title" text="Search Results"/>-->
					<!--		</content>-->
					<!--	</Toolbar>-->
					<!--</ui:toolbar>-->
					<ui:columns>
						<ui:Column width="11rem">
							<Label text="Material No"/>
							<ui:template>
								<Text text="{oBulkUploadModel>materialNumber}" wrapping="false"/>
							</ui:template>
						</ui:Column>
						<ui:Column width="11rem">
							<Label text="Commodity"/>
							<ui:template>
								<Text text="{oBulkUploadModel>commodity}" wrapping="false"/>
							</ui:template>
						</ui:Column>
						<ui:Column width="6rem" hAlign="End">
							<Label text="Sub Commodity"/>
							<ui:template>
								<Text text="{oBulkUploadModel>subcommodity}" wrapping="false"/>
							</ui:template>
						</ui:Column>
						<ui:Column width="9rem">
							<Label text="Attribute 1"/>
							<ui:template>
								<Text text="{oBulkUploadModel>attribute1}" wrapping="false"/>
							</ui:template>
						</ui:Column>
						<ui:Column width="9rem">
							<Label text="Attribute 2"/>
							<ui:template>
								<Text text="{oBulkUploadModel>attribute2}" wrapping="false"/>
							</ui:template>
						</ui:Column>
						<ui:Column width="9rem">
							<Label text="Attribute 3"/>
							<ui:template>
								<Text text="{oBulkUploadModel>attribute3}" wrapping="false"/>
							</ui:template>
						</ui:Column>
						<ui:Column width="9rem">
							<Label text="Attribute 4"/>
							<ui:template>
								<Text text="{oBulkUploadModel>attribute4}" wrapping="false"/>
							</ui:template>
						</ui:Column>
						<ui:Column width="9rem">
							<Label text="Base Price"/>
							<ui:template>
								<Text text="{oBulkUploadModel>basePrice}" wrapping="false"/>
							</ui:template>
						</ui:Column>
						<ui:Column width="9rem">
							<Label text="Historic Cost"/>
							<ui:template>
								<Text text="{oBulkUploadModel>historicalPrice}" wrapping="false"/>
							</ui:template>
						</ui:Column>
						<ui:Column width="9rem">
							<Label text="Forecast Cost"/>
							<ui:template>
								<Text text="{oBulkUploadModel>foreCastPrice}" wrapping="false"/>
							</ui:template>
						</ui:Column>
						<ui:Column width="9rem">
							<Label text="Appoved CMM Price"/>
							<ui:template>
								<Text text="{oBulkUploadModel>approvedCMMPrice}" wrapping="false"/>
							</ui:template>
						</ui:Column>
						<ui:Column width="9rem">
							<Label text="CMM Suggested Margin"/>
							<ui:template>
								<Text text="{oBulkUploadModel>CMMsuggestedMargin}" wrapping="false"/>
							</ui:template>
						</ui:Column>
						<ui:Column width="9rem">
							<Label text="CMM Proposed Margin"/>
							<ui:template>
								<Input value="{oBulkUploadModel>CMMproposedMargin}"/>
							</ui:template>
						</ui:Column>
						<ui:Column width="9rem">
							<Label text="CMM Proposed Price"/>
							<ui:template>
								<Input value="{oBulkUploadModel>CMMproposedPrice}"/>
							</ui:template>
						</ui:Column>
						<ui:Column width="9rem">
							<Label text="CMM Comments"/>
							<ui:template>
								<ComboBox></ComboBox>
							</ui:template>
						</ui:Column>
						<ui:Column width="9rem">
							<Label text="CMM Notes"/>
							<ui:template>
								<Input value="{oBulkUploadModel>CMMnotes}"/>
							</ui:template>
						</ui:Column>
					</ui:columns>
				</ui:Table> </Page>
			</pages>
		</App>
	</mvc:View>