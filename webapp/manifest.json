{
	"_version": "1.7.0",
	"sap.app": {
		"id": "jabilpocui",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"fullWidth": true,
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "JabilPoc.Jabil_Poc.view.approverScreen",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "JabilPoc.Jabil_Poc.i18n.i18n"
				}
			},
			"oMatTableDataModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"elemVisibilityModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"oApproverDataModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"oMarginDataModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"oUserModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"oMarginAllDataModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"oTaskContextDataModel": {
				"type": "sap.ui.model.json.JSONModel"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.2.0"
	}
}