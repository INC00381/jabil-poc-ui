{
	"_version": "1.7.0",
	"sap.app": {
		"id": "JabilPoc.Jabil_Poc",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"fullWidth": true,
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "JabilPoc.Jabil_Poc.view.View1",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "JabilPoc.Jabil_Poc.i18n.i18n"
				}
			},
			"oMatTableDataModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"elemVisibilityModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"oBulkUploadModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"oMarginDataModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"oUserModel": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"oMarginAllDataModel": {
				"type": "sap.ui.model.json.JSONModel"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "JabilPoc.Jabil_Poc.view",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"name": "requestScreen",
					"pattern": "",
					"target": [
						"requestScreen"
					]
				},
				{
					"name": "marginMaintenance",
					"pattern": "marginMaintenance",
					"target": [
						"marginMaintenance"
					]
				},
				{
					"name": "approverScreen",
					"pattern": "approverScreen",
					"target": [
						"approverScreen"
					]
				},
				{
					"name": "Reports",
					"pattern": "Reports",
					"target": [
						"Reports"
					]
				},
				{
					"name": "inbox",
					"pattern": "inbox",
					"target": [
						"inbox"
					]
				}
			],
			"targets": {
				"requestScreen": {
					"viewName": "requestScreen",
					"controlAggregation": "pages",
					"routerClass": "sap.m.routing.Router",
					"controlId": "mainContents"
				},
				"approverScreen": {
					"viewType": "XML",
					"viewName": "approverScreen",
					"routerClass": "sap.m.routing.Router",
					"controlId": "mainContents"
				},
				"Reports": {
					"viewType": "XML",
					"viewName": "Reports",
					"routerClass": "sap.m.routing.Router",
					"controlId": "mainContents"
				},
				"inbox": {
					"viewType": "XML",
					"viewName": "inbox",
					"routerClass": "sap.m.routing.Router",
					"controlId": "mainContents"
				},
				"marginMaintenance": {
					"viewType": "XML",
					"viewName": "marginMaintenance",
					"routerClass": "sap.m.routing.Router",
					"controlId": "mainContents"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.2.0"
	}
}