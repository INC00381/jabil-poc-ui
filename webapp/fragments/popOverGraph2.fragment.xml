<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
	<Popover showHeader="true" contentWidth="auto" placement="Top">
		<customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<HBox>
					<Label text="Inventory Info" class="popOverDateLabel"></Label>
				</HBox>
				<ToolbarSpacer/>
				<Button icon="sap-icon://decline" press="onClosePopOver2"/>
			</Toolbar>
		</customHeader>
		<content>
			<HBox>
				<VBox class="popOverVboxClass">
					<Label text="Commodity: " class="popOverLabel"></Label>
					<Label text="Sub-Commodity: " class="popOverLabel"></Label>
					<Label text="SAP Material No: " class="popOverLabel"/>
					<Label text="Stock: " class="popOverLabel"></Label>
					<Label text="Date: " class="popOverLabel"></Label>
				</VBox>
				<VBox class="popOverVboxClass">
					<Label text="CAPACITOR-CERAMIC" class="popOverLabel"></Label>
					<Label text="RESISTOR" class="popOverLabel"></Label>
					<Label text="11-0985-01" class="popOverLabel"/>
					<Label text="{oPriceTrendGraphModel>/stockLevel}" class="popOverLabel"></Label>
					<Label text="{oPriceTrendGraphModel>/stockDate}" class="popOverLabel"></Label>
				</VBox>
			</HBox>
		</content>
	</Popover>
</core:FragmentDefinition>